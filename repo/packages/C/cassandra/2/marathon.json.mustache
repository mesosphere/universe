,"EXECUTOR_JRE_LOCATION":"{{executor.jreLocation}}"
,"EXECUTOR_LOCATION":"{{executor.executorLocation}}"
,"EXECUTOR_CASSANDRA_LOCATION":"{{executor.cassandraLocation}}"
,"NODES":"{{nodes}}"
,"SEED_NODES":"{{seedNodes}}"
,"UPDATE_CONFIG":"{{updateConfig}}"
,"PLACEMENT_STRATEGY":"{{placementStrategy}}"
,"PLAN_STRATEGY":"{{planStrategy}}"
,"API_PORT":"{{apiPort}}"
,"ADMIN_PORT":"{{adminPort}}"
}
}
