{
  "id": "{{quobyte.framework-name}}",
  "cpus": {{quobyte.cpus}},
  "mem": {{quobyte.mem}},
  "instances": {{quobyte.instances}},
  "args": [
    "/opt/quobyte-mesos-cmd.sh",
    "--zk","{{quobyte.zk}}",
    "--master", "{{mesos.master}}",
    "--port", "{{quobyte.port}}",
    "--docker_image", "{{resource.assets.container.docker.f304cf7ccc31}}",
    "--framework_image", "{{resource.assets.container.docker.f8c203e427cc}}",
    "--registry_resources", "{{quobyte.registry_resources}}",
    "--metadata_resources", "{{quobyte.metadata_resources}}",
    "--data_resources", "{{quobyte.data_resources}}",
    "--api_resources", "{{quobyte.api_resources}}",
    "--webconsole_resources", "{{quobyte.webconsole_resources}}"],
  "env":{
    },
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "{{resource.assets.container.docker.f8c203e427cc}}",
      "forcePullImage": true,
      "network": "HOST"
    }
  },
  "healthChecks": [
    {
      "path": "/v1/health",
      "port": {{quobyte.port}},
      "protocol": "HTTP",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ],
  "labels": {
    "DCOS_PACKAGE_FRAMEWORK_NAME": "{{quobyte.framework-name}}"
  }
}
