{
  "id": "/{{scheduler.name}}/{{scheduler.user}}",
  "connected": false,
  "recovered": false,
  "TASK_UNREACHABLE": 0,
  "args": [
    "--zookeeperMesosUrl",
    "{{task.zookeeper}}",
    "--zookeeperMesosTimeout",
    "20000",
    "--webUiPort",
    "{{framework.webUiPort}}",
    "--frameworkFailoverTimeout",
    "2592000.0",
    "--frameworkName",
    "elasticsearch",
    "--elasticsearchNodes",
    "{{task.nodes}}",
    "--elasticsearchClusterName",
    "mesos-ha",
    "--elasticsearchCpu",
    "{{task.cpus}}",
    "--elasticsearchDisk",
    "{{task.disk}}",
    "--elasticsearchRam",
    "{{task.mem}}",
    "--executorForcePullImage",
    "true",
    "--executorName",
    "elasticsearch-executor",
    "--elasticsearchDockerImage",
    "{{resource.assets.container.docker.elasticsearch-task}}",
    "--useIpAddress",
    "true"
  ],
  "instances": {{scheduler.instances}},
  "cpus": {{scheduler.cpus}},
  "mem": {{scheduler.mem}},
  "disk": 0,
  "gpus": 0,
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "{{resource.assets.container.docker.elasticsearch-scheduler}}",
      "network": "HOST",
      "privileged": false,
      "forcePullImage": true
    }
  },
  "ports":[{{framework.webUiPort}}],
  "requirePorts":true,
  "healthChecks": [
    {
      "gracePeriodSeconds": 120,
      "intervalSeconds": 30,
      "timeoutSeconds": 5,
      "maxConsecutiveFailures": 0,
      "portIndex": 0,
      "path": "/",
      "protocol": "HTTP",
      "ignoreHttp1xx": false
    }
  ],
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 1
  },
  "unreachableStrategy": {
    "inactiveAfterSeconds": 300,
    "expungeAfterSeconds": 600
  },
  "killSelection": "YOUNGEST_FIRST",
  "requirePorts": true,
  "labels": {
    "DCOS_PACKAGE_RELEASE": "2",
    "DCOS_PACKAGE_SOURCE": "https://universe.mesosphere.com/repo",
    "DCOS_PACKAGE_METADATA": "",
    "DCOS_PACKAGE_REGISTRY_VERSION": "3.0",
    "DCOS_PACKAGE_FRAMEWORK_NAME": "elasticsearch",
    "DCOS_PACKAGE_VERSION": "1.0.1-2",
    "DCOS_PACKAGE_NAME": "elasticsearch",
    "DCOS_PACKAGE_IS_FRAMEWORK": "true"
  },
  "env": {
    "JAVA_OPTS": "{{scheduler.java_opts}}"
  }
}
